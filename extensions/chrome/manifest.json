{
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "Google Meet Attendees"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["js/copy.simple.user.js"],
      "matches": ["https://meet.google.com/*"],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "manifest_version": 3,
  "homepage_url": "https://github.com/C4illin/Google-Meet-Attendees",
  "name": "__MSG_extName__",
  "short_name": "__MSG_appName__",
  "version": "0.2.5",
  "web_accessible_resources": [
    {
      "resources": ["js/copy.simple.user.js"],
      "matches": ["https://meet.google.com/*"]
    }
  ]
}
